{
  "name": "asp-example",
  "version": "1.0.0",
  "description": "Example implementation of Agent Swarm Protocol",
  "main": "orchestrator/index.js",
  "scripts": {
    "start": "node orchestrator/index.js",
    "start:agent:conversation": "node agents/conversation-agent/index.js",
    "start:agent:research": "node agents/research-agent/index.js",
    "start:agent:summarization": "node agents/summarization-agent/index.js",
    "sdk:example:conversation": "node sdk/examples/run-conversation-agent.js",
    "sdk:example:research": "node sdk/examples/run-research-agent.js",
    "sdk:example:client": "node sdk/examples/run-client-sdk.js",
    "client:terminal": "node clients/terminal-client.js",
    "client:install": "cd clients && npm install && npm link",
    "test:sdk": "node -e \"require('./sdk')\""
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "node-fetch": "^2.6.7",
    "uuid": "^9.0.0",
    "ws": "^8.12.0"
  }
} 